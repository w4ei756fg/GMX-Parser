    <event eventtype="9" enumb="79">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if keyboard_check(vk_control)
{
    objname = get_open_filename("GameMaker Object files|*.object.gmx", "");
    
    
    if (objname != "")
    {
        //file load
        var file = file_text_open_read(objname), text_l = 0;
        while(!file_text_eof(file))
        {
            text[text_l] = file_text_read_string(file);
            file_text_readln(file);
            text_l++;
        }
        file_text_close(file);
        
        //check correct file
        if (text[0] == checksum)
        {
            //project.gmx parse
            objName = tag_parse(objname, 'objects\', '.object.gmx');
            eventtype = -1; // eventtype
            enumb = -1; // enumb
            code = 0; // &lt;string&gt;
            obj_c = 0; // for collision event
            for(var i = 2; i &lt; text_l; i++)
            {
                var current_text = text[i];
                if (eventtype == -1) // object setting
                {
                    for(var r = 0; r &lt; array_length_1d(obj_set); r++) // object setting
                    if string_count('&lt;' + obj_set[r] + '&gt;', current_text) &amp;&amp; string_count('&lt;/' + obj_set[r] + '&gt;', current_text)
                    {
                        ds_list_add(objSet, obj_set[r], tag_parse(current_text, '&lt;' + obj_set[r] + '&gt;', '&lt;/' + obj_set[r] + '&gt;'))
                        break;
                    }
                    
                    if string_count('&lt;event eventtype="', current_text) // event begin
                    {
                        eventtype = real(tag_parse(current_text, '&lt;event eventtype="', '" enumb="'));
                        if eventtype == 4 // collision event
                        {
                            enumb = obj_c;
                            event_init(eventtype, enumb);
                            event_add(eventtype, enumb, "obj", tag_parse(current_text, '" enumb="', '"&gt;'));
                            obj_c++;
                        }
                        else
                        {
                            enumb = real(tag_parse(current_text, '" enumb="', '"&gt;'));
                            event_init(eventtype, enumb);
                        }
                    }
                }
                else // event
                {
                
                    if code // code parse
                    {
                        if string_count('&lt;/string&gt;', current_text) // code block end
                        {
                            code = 0;
                        }
                        else
                        {
                            event_add(eventtype, enumb, 'code', filter_xml(current_text));
                        }
                    }
                    else // event option parse
                    {
                        for(var r = 0; r &lt; array_length_1d(obj_event); r++) // event option
                        if string_count('&lt;' + obj_event[r] + '&gt;', current_text) &amp;&amp; string_count('&lt;/' + obj_event[r] + '&gt;', current_text)
                        {
                            event_add(eventtype, enumb, obj_event[r], tag_parse(current_text, '&lt;' + obj_event[r] + '&gt;', '&lt;/' + obj_event[r] + '&gt;'));
                            break;
                        }
                        
                        if string_count('&lt;string&gt;', current_text) // code block begin
                        {
                            code = 1;
                            var pos = string_pos('&lt;string&gt;', current_text) + string_length('&lt;string&gt;');
                            event_add(eventtype, enumb, 'code', filter_xml(string_delete(current_text, 1, pos - 1)));
                        }
                        
                        if string_count('&lt;/event&gt;', current_text) // event end
                        {
                            eventtype = -1;
                            enumb = -1;
                        }
                    }
                }
            }
        }
        else
        {
            show_message("Incorrect file.");
        }
        
        //information display
        object_info();
    
    }
}
</string>
