draw_rectangle(x + w - scrw, y + menuh, x + w - 1, y + h - errh - infh - scrw - 1, 0); // bg
draw_set_color(ccc[(sClicked == 2)]);
draw_rectangle(x + w - scrw + 1, y + menuh + scrw + (h - menuh - errh - infh - scrw * 3 - bar_y) * a_sy / maxsy, 
