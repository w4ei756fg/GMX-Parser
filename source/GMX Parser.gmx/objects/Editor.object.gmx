    }
    for(var xx = 0; xx &lt; string_length(text[yy]); xx++)
    {
        if !(ord(string_char_at(col[yy], xx + 1)) - 97 &lt; 0) { coll = ord(string_char_at(col[yy], xx + 1)) - 97; } // set color
        draw_set_color(color[coll]);
        
        // normal text
