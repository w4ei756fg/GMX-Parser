maxsy = max(0, (text_l * lineh - (h - menuh - errh - infh - scrw)));
bar_x = (w - scrw * 2) * (w - scrw - linew) / (maxsx + w - scrw - linew);
bar_y = (h - menuh - errh - infh - scrw * 3) * (h - menuh - errh - infh - scrw) / (maxsy + h - menuh - errh - infh - scrw);
